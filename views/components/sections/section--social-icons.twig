<section class="section--social-icons flex flex--wrap justify--between">
{% if options.social.facebook != null %}<a href="{{options.social.facebook}}" target="_blank"><i class="icon-social--fb" data-grunticon-embed></i></a>{% endif %}
{% if options.social.twitter != null %}<a href="https://twitter.com/{{options.social.twitter}}" target="_blank"><i class="icon-social--tw" data-grunticon-embed></i></a>{% endif %}
{% if options.social.instagram != null %}<a href="https://www.instagram.com/{{options.social.instagram}}" target="_blank"><i class="icon-social--insta" data-grunticon-embed></i></a>{% endif %}
{% if options.social.linkedin != null %}<a href="{{options.social.linkedin}}" target="_blank"><i class="icon-social--in" data-grunticon-embed></i></a>{% endif %}
{% if options.social.youtube != null %}<a href="{{options.social.youtube}}" target="_blank"><i class="icon-social--yt" data-grunticon-embed></i></a>{% endif %}
</section>